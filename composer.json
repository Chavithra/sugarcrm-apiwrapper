{
    "name": "inetprocess/sugarcrm-apiwrapper",
    "description": "A very simple API Wrapper for SugarCRM 7",
    "type": "library",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Emmanuel Dyan",
            "email": "emmanuel.dyan@inetprocess.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "@stable",
        "webmozart/assert": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable"
    },
    "autoload": {
        "psr-4": {
            "InetProcess\\SugarAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InetProcess\\SugarAPI\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
           "php": "5.6"
        }
    }
}
